        "x": 0,
        "y": 0
      },
      "id": 100,
      "options": {
        "content": "# Binance Provider Overview\n\nBinance spot adapter observability: live order throughput, rejection tracking, latency, and stream reliability",
        "mode": "markdown"
      },
      "type": "text"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Binance Orders / sec",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 2
      },
      "id": 101,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "sum(rate(meltica_provider_binance_orders_received{environment=\"$environment\"}[1m]))",
          "refId": "A"
        }
      ],
      "title": "Binance Orders / sec",
      "type": "stat"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Binance Rejection %",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.1
              },
              {
                "color": "red",
                "value": 0.3
              }
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 2
      },
      "id": 102,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "100 * sum(rate(meltica_provider_binance_orders_rejected{environment=\"$environment\"}[5m])) / clamp_min(sum(rate(meltica_provider_binance_orders_received{environment=\"$environment\"}[5m])), 1e-3)",
          "refId": "A"
        }
      ],
      "title": "Binance Rejection %",
      "type": "stat"
    },
